{
  "name": "legc-lan-launcher",
  "version": "1.0.4-rc.4",
  "private": true,
  "description": "A launcher to provide content (e.g. games, but it can also be used for other stuff) in a P2P-manner to friends on a LAN-Party. It is no more than a frontend based on electron to control a slightly modified instance of Syncthing.",
  "author": {
    "name": "Julian Lehrhuber",
    "email": "jl@lerry.de"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "core-js": "^3.35.1",
    "hsl-to-hex": "^1.0.0",
    "material-icons": "^1.13.12",
    "roboto-fontface": "*",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@mdi/js": "^7.4.47",
    "electron": "^22.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.56.0",
    "sass": "^1.70.0",
    "sass-loader": "^13.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "GNU GPLv3",
  "productName": "LEGC LAN-Launcher"
}
