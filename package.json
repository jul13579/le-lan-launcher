{
  "name": "legc-lan-launcher",
  "version": "1.0.3-rc.1",
  "private": true,
  "description": "A launcher to provide content (e.g. games, but it can also be used for other stuff) in a P2P-manner to friends on a LAN-Party. It is no more than a frontend based on electron to control a slightly modified instance of Syncthing.",
  "author": {
    "name": "Julian Lehrhuber",
    "email": "jl@lerry.de"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build --publish=never",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^1.2.1",
    "chart.js": "^3.6.2",
    "core-js": "^3.26.1",
    "epic-spinners": "^1.1.0",
    "hsl-to-hex": "^1.0.0",
    "material-icons": "^1.13.1",
    "roboto-fontface": "*",
    "vue-i18n": "^8.26.7",
    "vue-toasted": "^1.1.28",
    "vuetify": "^2.6.13",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@mdi/js": "^7.1.96",
    "@volar-plugins/vetur": "^0.1.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-dom": "^3.2.45",
    "electron": "^22.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.30.0",
    "eslint-plugin-vue": "^9.8.0",
    "sass": "^1.57.0",
    "sass-loader": "^13.2.0",
    "vue": "^2.6.14",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-template-compiler": "^2.7.14",
    "vuetify-loader": "^1.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "GNU GPLv3",
  "productName": "LEGC LAN-Launcher"
}
